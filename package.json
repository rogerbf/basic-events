    "test": "echo 'no tests'",
    "start": "npm run build && node ./distribution/index.js",
    "watch:start": "onchange \"source/*.js\" \"source/**/*.js\" -- npm run start",
    "eslint": "eslint source",
    "eslint:fix": "eslint --fix source",
    "repl": "npm run build && babel-node",
    "snyk": "snyk test"
  },
  "keywords": [
    "event",
    "events",
    "eventemitter",
    "emitter",
    "pubsub"
