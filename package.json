{
  "name": "basic-events",
  "version": "1.0.0",
  "description": "a very basic event emitter",
  "main": "./distribution/index.js",
  "files": [
    "distribution",
    "README.md"
  ],
  "scripts": {
    "prebuild": "npm test && rimraf distribution",
    "build": "cross-env BABEL_ENV=production babel --out-dir distribution source",
